name = MoPub

# The location of the Flex SDK 
flex.sdk = path/to/your/flex/SDK

# Executable files extensions (for Mac leave empty, for Windows, set to .exe and .bat)
bin.ext = .exe
bat.ext = .bat

# The javac executable folder
javac.dir = path/to/java/bin

# Location of the MoPub Android SDK 
mopub.android.sdk = path/to/mopub-android-sdk

# Location of the Android SDK (folder containing android.jar)
android.sdk = path/to/android.jar/folder